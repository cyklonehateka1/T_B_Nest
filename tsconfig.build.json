{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noEmitOnError": false,
    "isolatedModules": false
  },
  "exclude": [
    "node_modules", 
    "prod_node_modules",
    "test", 
    "dist", 
    "**/*spec.ts",
    "**/*.e2e-spec.ts"
  ]
}